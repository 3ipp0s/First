<!DOCTYPE html>
<html>
  <head>
    <meta charset="utf-8">
    <!-- PAGE TITLE -->
    <title>EZZkatalog - place your things</title>
    <!-- MAKE IT RESPONSIVE -->
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <!-- BOOTSTRAP -->
    <%= stylesheet_link_tag 'bootstrap.min.css', :media => 'screen'%>
    <!-- <link href="css/bootstrap.min.css" rel="stylesheet" media="screen"> -->
    <!-- MAIN STYLE -->
    <%= stylesheet_link_tag 'customize.css', :media => 'screen'%>
    <!-- <link href="css/customize.css" rel="stylesheet" media="screen"> -->
    <%= stylesheet_link_tag 'style.css', :media => 'screen'%>
    <!-- <link href="style.css" rel="stylesheet" media="screen"> -->
    <!-- FONTS -->
    <%= stylesheet_link_tag 'http://fonts.googleapis.com/css?family=Lato:300,400,700', :type => 'text/css'%>
    <!-- <link href='http://fonts.googleapis.com/css?family=Lato:300,400,700' rel='stylesheet' type='text/css'> -->
    <%= stylesheet_link_tag 'http://fonts.googleapis.com/css?family=Varela+Round', :type => 'text/css'%>
    <!-- <link href='http://fonts.googleapis.com/css?family=Varela+Round' rel='stylesheet' type='text/css'> -->
    <!-- HTML5 shim and Respond.js IE8 support of HTML5 elements and media queries -->
    <!--[if lt IE 9]>
      <script src="js/html5shiv.js"></script>
      <script src="js/respond.min.js"></script>
    <![endif]-->
  </head>
  <!-- START BODY -->
  <body>
  <div id="page">
    <!-- START MAIN CONTAINER -->
    <div id="main-container">


      <% flash.each do |message_type, message| %>
       <%= content_tag(:div, message, class: "alert alert-#{message_type}") %>
      <% end %>
      <%= yield %>

    </div>
    <!-- END MAIN CONTAINER -->
    
    <!-- PAGE LOADING-->
    <div id="loader"></div>
    </div>
    <!-- SCRIPTS -->
    <!-- <script src="http://code.jquery.com/jquery.js"></script> -->
    <%= javascript_include_tag 'http://code.jquery.com/jquery.js'%>
  <!-- <script src="js/alert.js"></script> -->
  <%= javascript_include_tag 'alert.js'%>
 <!--  <script src="js/jquery.sequence-min.js"></script> -->
  <%= javascript_include_tag 'jquery.sequence-min.js'%>
   <!--  <script src="js/jquery.fancybox.pack.js"></script> -->
    <%= javascript_include_tag 'jquery.fancybox.pack.js'%>
    <!-- <script src="js/jquery.scrollUp.min.js"></script> -->
    <%= javascript_include_tag 'jquery.scrollUp.min.js'%>
    <!-- <script src="js/jquery.smoothscroll.min.js"></script> -->
    <%= javascript_include_tag 'jquery.smoothscroll.min.js'%>
    <!-- <script src="js/jquery.meanmenu.min.js"></script> -->
    <%= javascript_include_tag 'jquery.meanmenu.min.js'%>
    <!-- <script src="js/pace.min.js"></script> -->
    <%= javascript_include_tag 'pace.min.js'%>
    <!-- <script src="js/jquery.flexslider-min.js"></script> -->
    <%= javascript_include_tag 'jquery.flexslider-min.js'%>
    <!-- <script src="js/custom.js"></script> -->
    <%= javascript_include_tag 'custom.js'%>
  </body>
  <!-- END BODY -->
</html>